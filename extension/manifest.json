{
  "name": "Gmail.js",
  "version": "0.1",
  "description": "Hello World Gmail chrome extension",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "js/jquery-1.9.1.min.js",
    "js/gmail.js",
    "src/main.js"
  ],
  "manifest_version": 2
}
